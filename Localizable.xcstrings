        "ja" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "不等式とは？"
          }
        }
      }
    },
    "step4.description" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "The numbers on the blocks look different from the front and back. Let’s walk around to see all sides."
          }
        },
        "ja" : {
          "stringUnit" : {
            "state" : "needs_review",
            "value" : "ブロックに書かれた数字は正面と背面で異なるので\n全ての面を見るにはブロックの周りを歩いて確認しよう！"
          }
        }
      }
    },
    "step4.title" : {
